dict = {0:'A', 1:'B', 2:'C', 3:'D', 4:'E'}
ret = [["测试点","$T$","$n$","$a_i$","特殊性质"]]
for datum in prob['data']:	
	args = datum['args']
	row = [
		tl.cases(datum['cases']),			
		r"$\le %s$" % tl.hn(args['T']),		
		r"$\le %s$" % tl.hn(args['n']),		
		r"$\le %s$" % ("10^9" if args['v'] == 1 else "n"), 
		no_merge(dict[args['spc']] if args['spc'] != -1 else "无")
	]
	ret.append(row)	
base.log.debug(u'输出调试信息')
return ret
